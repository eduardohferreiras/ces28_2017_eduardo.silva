package exec02;

public abstract class Person {

	Person(String name, Phone phone, String email, String gender)
	{
		name_ = name;
		phone_ = phone;
		email_ = email;
		gender_ = gender;
	}
	
	protected String name_;
	protected Phone phone_;
	protected String email_;
	protected String gender_;
	
	public String name() {
		return null;
	}

	public Phone phone() {
		return phone_;
	}

	public String email() {
		return email_;
	}

}
