package exec11;

public class UserInterface {
	
	static int idglobal = 1;
	private int id;
	private Usuario _subject;
	UserInterface(Usuario user){
		id = idglobal++;
		_subject = user;
	}
	
	public void update() {
		System.out.println("Obs" + id + " notificado\n");
		System.out.println(_subject.getState()); VB/F
	}
}
